rem/ Mini menu onglet Elie
ccp/ /fix.niveau = 2

fenetre/ MINI_ELIE
                .titre                 = "ElieOS option"
		.px				= "0"
		.py				= "25"
		.tx				= "169"
		.ty				= "45"
		rem/.Opacite 		= "150"
