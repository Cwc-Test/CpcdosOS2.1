picturebox/ veille
	.Handle			= "%roll_screen%"
	.Parametres		= "IMGAUTO:2"
	.PX			= "110"
	.PY			= "2"
	.tx			= "40"
	.ty		        = "40"
	.Image			= "OS/ELIEOS/SYSTEM4/ELIEXT/ELIELDR/MEDIA/GUI/ICONES/OS/veille.png"
	rem/ .opacite		= "150"
	.event			= "%_exe_path_%=Click"
	creer/
Fin/ Picturebox

picturebox/ btn_stop
	.Handle			= "%roll_screen%"
	.Parametres		= "IMGAUTO:2"
	.PX			= "60"
	.PY			= "2"
	.tx			= "40"
	.ty		        = "40"
	.Image			= "OS/ELIEOS/SYSTEM4/ELIEXT/ELIELDR/MEDIA/GUI/ICONES/OS/shut.png"
	rem/ .opacite		= "150"
	.event			= "%_exe_path_%=Click"
	creer/
Fin/ Picturebox

picturebox/ btn_rest
	.Handle			= "%roll_screen%"
	.Parametres		= "IMGAUTO:2"
	.PX			= "10"
	.PY			= "2"
	.tx			= "40"
	.ty		        = "40"
