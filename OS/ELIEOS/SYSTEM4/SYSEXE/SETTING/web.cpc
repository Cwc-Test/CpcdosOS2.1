TEXTEBLOC/ id_cpu
	.Handle = "%dos_sys%"
	.PARAMETRES = "COL:0, BORD:0"
	.TEXTE = "Do you want to activate web services?"
	.PX = "20"
	.PY = "3"
	.COULEURTEXTE = "000,000,000"
	CREER/
FIN/ TEXTEBLOC

Bouton/ btn_yes
	.Handle			= "%dos_sys%"
	.Parametres		= "IMGAUTO:2"
	.PX				= "15"
	.PY				= "25"
	.tx				= "75"
	.ty				= "40"
	rem/ .opacite		= "100"
	.texte			= "Activate"
	.Image			= "OS/ELIEOS/SYSTEM4/ADVANCE/GUI/Buttons/BTN_BLUE.png"
	.event			= "%_exe_path_%=Click"
	creer/
Fin/ Bouton

Bouton/ btn_no
	.Handle			= "%dos_sys%"
	.Parametres		= "IMGAUTO:2"
	.PX				= "110"
	.PY				= "25"
	.tx				= "85"
	.ty				= "40"
	rem/ .opacite		= "100"
	.texte			= "Deactivate"
