	.tx 			= "390"
	.ty = "100"
	// .opacite 	= "100"
	.parametres 	= "TYPE:0"
	.CouleurFenetre	= "050,050,100"
	.CouleurFond	= "195,195,195"
	.event = ""
	@#dos_sys creer/
